<%= form_tag @search_form_path, :method => 'get', :class => "form-inline", :style => "float: right;" do %> 
  <div class="input-group-sm">
    <%= text_field_tag :license_name, params[:license_name], :class => "search-query form-control" %>
    <button type="submit" class="btn btn-sm btn-default"><i class="glyphicon glyphicon-search"></i> <%= t('activerecord.models.license') %></button>
  </div>
<% end %>