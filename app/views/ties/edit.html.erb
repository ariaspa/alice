<div class="container-fluid">
<div class="row-fluid">

<div class="span6">
<div class="well sidebar-nav">
<!--Product content-->
<ul class="nav nav-list">
  <li class="nav-header"><%= t('activerecord.models.products') %></li>
  <li class="divider"></li>
  <li class="active"><a href="#"><%= @product.name %> - Vers. <%= @product.version %></li></a>
</ul>
</div><!--/.well -->
</div><!--/span-->

<div class="span6">
	<%= render :partial => "search_component" %>
<div class="well sidebar-nav">
<!--Component content-->
<ul class="nav nav-list">
  <li class="nav-header"><%= t('activerecord.models.components') %></li>
  <li class="divider"></li>
  <% @components.each do |component| %>
    <li>
      <label class="checkbox inline">
        <input type="checkbox" id="inlineCheckbox1" value="checked">
          <%= link_to component.name+" - Vers. "+component.version, component_ties_select_path(component) %>
     </label>  
    </li>
  <% end %>
</div><!--/.well -->
</div><!--/span-->

</div>
</div>

<br />
<%= link_to ties_index_path, :class => "btn", :title => t('actions.back'), :alt => t('actions.back') do %>
      <i class="icon-arrow-left"></i> <%= t('actions.back') %>      
<% end %>  
    
    

