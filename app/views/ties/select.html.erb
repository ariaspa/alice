<div class="container-fluid">
<div class="row-fluid">

<div class="span6">
<div class="well sidebar-nav">
<!--Product content-->
<ul class="nav nav-list">
  <li class="nav-header"><%= t('activerecord.models.products') %></li>
  <% @products.each do |product| %>
    <li><%= link_to product.name, product_ties_select_path(product) %></li>
  <% end %>
</ul>
</div><!--/.well -->
</div><!--/span-->

<div class="span6">
<div class="well sidebar-nav">
<!--Component content-->
<ul class="nav nav-list">
  <li class="nav-header"><%= t('activerecord.models.components') %></li>
  <% @components.each do |component| %>
    <li><%= link_to component.name, component_ties_select_path(component) %></li>
  <% end %>
</div><!--/.well -->
</div><!--/span-->

</div>
</div>
